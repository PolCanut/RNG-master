<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RNG.vista.GestionOSController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Label layoutX="246.0" layoutY="55.0" prefHeight="32.0" prefWidth="149.0" text="Nuevo Articulo" textAlignment="CENTER">
                    <font>
                        <Font size="22.0" />
                    </font>
                </Label>
                <Label layoutX="64.0" layoutY="135.0" prefHeight="17.0" prefWidth="132.0" text="Código del articulo:" />
                <Label layoutX="64.0" layoutY="173.0" prefHeight="17.0" prefWidth="132.0" text="Descripción del articulo:" />

                <TextField fx:id="CajaCodigo" layoutX="213.0" layoutY="131.0" prefHeight="25.0" prefWidth="324.0" />
                <TextField fx:id="CajaDescripcion" layoutX="213.0" layoutY="169.0" prefHeight="25.0" prefWidth="324.0" />
                <TextField fx:id="CajaPrecio" layoutX="213.0" layoutY="205.0" prefHeight="25.0" prefWidth="324.0" />
                <HBox layoutX="248.0" layoutY="324.0">
                    <children>
                        <Button fx:id="GuardarArticulo" mnemonicParsing="false" text="Guardar Articulo" onAction="#guardarArticulo" />
